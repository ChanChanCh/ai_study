package day12;

public class Quiz5 {

	public static void main(String[] args) {

		int sum = 0;
		int count = 1;
			
			for(int i =0; i >1000; i++) {
				if(count % 2 ==1) {
					sum += count;
				}
				
				if(sum > 1000) {
					System.out.println("1000을 넘어설 때의 홀수 :" + count);
					break;
				}
				count++;
			}
	}
}


